<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="PresetValueButton">
    <attr format="integer" name="presetButtonValueText"/>
    <attr format="string" name="presetButtonUnitText"/>
    <attr format="color" name="presetButtonValueTextColor"/>
    <attr format="color" name="presetButtonPressedTextColor"/>
    <attr format="color" name="presetButtonUnitTextColor"/>
  </declare-styleable>

  <declare-styleable name="PresetRadioGroup">
    <attr format="reference" name="presetRadioCheckedId"/>
  </declare-styleable>

  <declare-styleable name="RangeSeekBar" >
    <attr  format="integer" name="absoluteMinValue"/>
    <attr  format="integer" name="absoluteMaxValue"/>
    <attr  format="boolean" name="singleThumb"/>
  </declare-styleable>


</resources>